{
  "name": "jabberwock",
  "version": "0.0.1",
  "description": "The best editor in the world",
  "main": "src/index.ts",
  "scripts": {
    "dev": "webpack-dev-server",
    "devo": "webpack-dev-server --open",
    "build": "webpack",
    "tsc:dev": "concurrently \"tsc -w -p tsconfig-amd.json \" \"node dev/server.js dev/index-amd.html\"",
    "tsc:build": "tsc --preserveSymlinks -p tsconfig-amd.json",
    "tsc:dev-esm": "concurrently \"tsc -w -p tsconfig-es-modules.json\" \"node dev/server.js dev/index-es-modules.html\"",
    "tsc:build-esm": "tsc -p tsconfig-es-modules.json",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odoo/jabberwock.git"
  },
  "author": "MultiAuthor",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/odoo/jabberwock/issues"
  },
  "homepage": "https://github.com/odoo/jabberwock#readme",
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/dom-inputevent": "^1.0.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "acorn": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "finalhandler": "^1.1.2",
    "karma": "^4.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "serve-static": "^1.14.1",
    "sinon": "^7.4.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
